{
  "root": { "$type": "dimension", "$value": 16 },
  "multiplier": { "$type": "dimension", "$value": 1.25 },
  "rootMultiplier": { "$type": "dimension", "$value": "{root} * {multiplier}" },

  "typescale": {
    "lg": { "$type": "dimension", "$value": 1.333 },
    "md": { "$type": "dimension", "$value": 1.333 },
    "sm": { "$type": "dimension", "$value": 1.25 },
    "horiz": { "$type": "dimension", "$value": 1.067 }
  },

  "breakpoints": {
      "desktop": { "$type": "sizing", "$value": 1280 },
      "tablet": { "$type": "sizing", "$value": 800 },
      "mobile": { "$type": "sizing", "$value": 320 }
  },

