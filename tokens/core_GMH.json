{
  "root": { "$type": "dimension", "$value": 16 },
  "multiplier": { "$type": "dimension", "$value": 1.25 },
  "rootMultiplier": { "$type": "dimension", "$value": "{root} * {multiplier}" },

  "typescale": {
    "lg": { "$type": "dimension", "$value": 1.333 },
    "md": { "$type": "dimension", "$value": 1.333 },
    "sm": { "$type": "dimension", "$value": 1.25 },
    "horiz": { "$type": "dimension", "$value": 1.067 }
  },

  "breakpoints": {
      "desktop": { "$type": "sizing", "$value": 1280 },
      "tablet": { "$type": "sizing", "$value": 800 },
      "mobile": { "$type": "sizing", "$value": 320 }
  },

  "color": {
    "black": {
      "100%": { "$type": "color", "$value": "#000000FF" },
      "90%": { "$type": "color", "$value": "#000000E5" },      
      "75%": { "$type": "color", "$value": "#000000BF" },
      "50%": { "$type": "color", "$value": "#0000007F" },
      "30%": { "$type": "color", "$value": "#0000004D" },
      "25%": { "$type": "color", "$value": "#0000003F" },
      "15%": { "$type": "color", "$value": "#00000026" },
      "10%": { "$type": "color", "$value": "#0000001A" },
      "5%": { "$type": "color", "$value": "#0000000D" },
      "2%": { "$type": "color", "$value": "#00000005" },
      "1%": { "$type": "color", "$value": "#00000002" },
      "0%": { "$type": "color", "$value": "#00000000" }
    },
    "white": {
      "100%": { "$type": "color", "$value": "#FFFFFFFF" },
      "90%": { "$type": "color", "$value": "#FFFFFFE5" },
      "75%": { "$type": "color", "$value": "#FFFFFFBF" },
      "50%": { "$type": "color", "$value": "#FFFFFF7F" },
      "30%": { "$type": "color", "$value": "#FFFFFF4D" },
      "25%": { "$type": "color", "$value": "#FFFFFF3F" },
      "15%": { "$type": "color", "$value": "#FFFFFF26" },
      "10%": { "$type": "color", "$value": "#FFFFFF1A" },
      "5%": { "$type": "color", "$value": "#FFFFFF0D" },
      "2%": { "$type": "color", "$value": "#FFFFFF05" },
      "1%": { "$type": "color", "$value": "#FFFFFF02" },
      "0%": { "$type": "color", "$value": "#FFFFFF00" }
    }
  }
}